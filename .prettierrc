{
  "tabWidth": 2,
  "semi": false,
  "singleQuote": true,
  "trailingComma": "es5",
  "tailwindConfig": "./tailwind.config.js",
  "printWidth": 100,
  "importOrder": [
    "^@nest(.*)$",
    "<THIRD_PARTY_MODULES>",
    "^@app/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrderParserPlugins": ["decorators-legacy", "jsx", "typescript"]
}
